arr = [10,9,4,5,2,1,3,8]
arr.sort()
def binary_search(arr,target):
    lb=0
    ub = len(arr)-1
    while lb<=ub:
        mid = lb + (ub-lb)//2
        if arr[mid]==target:
            print("element is found at index", mid)
            return mid
        elif arr[mid]<target:
            lb = mid+1
        else:
            ub = mid-1
    print("element is not found")
    return-1
binary_search(arr,5)
